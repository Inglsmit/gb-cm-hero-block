{
	"apiVersion": 2,
	"name": "cm-block/hero-block",
	"title": "Hero block",
	"category": "widgets",
	"icon": "smiley",
	"description": "CM hero block - image, title, subtitle.",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h1"
		},
		"description": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"id": {
			"type": "number"
		},
		"alt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"btnTitle": {
			"type": "string",
			"source": "html",
			"selector": "a"
		},
		"btnUrl": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href"
		}
	},
	"textdomain": "hero-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}